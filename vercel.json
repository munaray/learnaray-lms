{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/v1"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
}
