{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api-docs/swagger-ui/(.*)",
      "dest": "/public/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
}
